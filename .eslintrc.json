{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "error",
        "camelcase": "error",
        "no-duplicate-imports":"error",
        "line-comment-position":"error",
        "no-multiple-empty-lines": "error",
        "no-empty-character-class": "error"
    }
}
